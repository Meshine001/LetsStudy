<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <com.meshine.letsstudyclient.widget.TopBarView
        android:layout_alignParentTop="true"
        android:id="@+id/id_lets_run_topbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/topbar_height"
        app:centerText="@string/lets_sport"
        app:isShowLeft="true"
        app:isShowRight="false"
        app:isShowWarn="false"
        app:isBack = "true"
        app:leftSrc="@drawable/ic_back"/>
    <com.baidu.mapapi.map.MapView
        android:id="@+id/id_lets_run_map"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:clickable="true" />

</LinearLayout>
